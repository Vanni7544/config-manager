{
  "name": "vanni/config-manager",
  "version": "1.1.0",
  "description": "Environment configuration manager for Laravel",
  "type": "library",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "Vanni\\ConfigManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vanni\\ConfigManager\\ConfigManagerServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "livewire/livewire": "^3.0"
  },
  "archive": {
        "exclude": [
            "/composer.lock",
            "/.DS_Store",
            "/vendor",
            "/tests",
            "/*.log"
        ]
    }
}
